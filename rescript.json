{
  "name": "redemon-ui",
  "sources": {
    "dir": "shared",
    "subdirs": true
  },
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".res.mjs",
  "bs-dependencies": [
    "@rescript/core",
    "@greenlabs/ppx-spice"
  ],
  "bsc-flags": [
    "-open RescriptCore"
  ],
  "ppx-flags": [
    "@greenlabs/ppx-spice/ppx"
  ],
  "gentypeconfig": {
    "module": "esmodule",
    "moduleResolution": "bundler",
    "generatedFileExtension": ".gen.tsx",
    "shims": {
      "RescriptPervasives": "RescriptPervasives"
    }
  }
}
